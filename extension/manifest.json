{
  "manifest_version": 3,
  "name": "Vom Platzl",
  "version": "0.1.0",
  "description": "Reimpower Local Stores",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": [
    "https://www.google.com/*",
    "https://maps.googleapis.com/*",
    "http://localhost:3000/*",
    "http://127.0.0.1:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*", "http://www.google.com/*"],
      "js": ["config.js", "content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["query_analysis.js", "Silhouette_of_Munich.svg-removebg-preview.png", "logo.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "logo-128.png"
  }
}
